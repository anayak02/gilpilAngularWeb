{
  "name": "gilpil-angular-web",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build:prod": "ng build --prod --configuration=production --aot=false --buildOptimizer=false --deleteOutputPath=true",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start-springboot": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --aot=false --buildOptimizer=false --deleteOutputPath=true",
    "build:tomcat:root": "ng build  --aot=false --buildOptimizer=false --deleteOutputPath=true",
    "build:baseurl": "ng build --baseHref=/gilpilWeb/ --aot=false --buildOptimizer=false --deleteOutputPath=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "sudo cp -r * /var/lib/tomcat7/webapps/ROOT/",
    "use": "nvm use v13.5.0",
    "devkit": "npm install --save-dev @angular-devkit/build-angular",
    "primeng": "npm install primeng --save",
    "primengicon": "npm install primeicons --save"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.6",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.1.6",
    "@angular/compiler": "~10.1.6",
    "@angular/core": "~10.1.6",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~10.1.6",
    "@angular/material": "^10.2.7",
    "@angular/material-moment-adapter": "8.2.1",
    "@angular/platform-browser": "~10.1.6",
    "@angular/platform-browser-dynamic": "~10.1.6",
    "@angular/router": "~10.1.6",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "ag-grid-angular": "^25.3.0",
    "ag-grid-community": "^25.3.0",
    "angular-datatables": "^10.0.0",
    "datatables.net": "^1.10.22",
    "datatables.net-dt": "^1.10.22",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "moment": "2.24.0",
    "ngx-spinner": "^10.0.1",
    "ngx-toastr": "^13.2.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.7",
    "@angular/cli": "~10.1.7",
    "@angular/compiler-cli": "~10.1.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
